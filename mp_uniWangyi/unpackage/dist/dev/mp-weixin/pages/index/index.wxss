#indexWrap {
  width: 100%;
  height: 100%;
}
#indexWrap .header-wrap {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
  background-color: #fff;
}
#indexWrap .header-wrap .nav-list {
  width: 100%;
  height: 80rpx;
  border-bottom: 1rpx solid #eee;
  white-space: nowrap;
}
#indexWrap .header-wrap .nav-list .nav-item {
  width: 140rpx;
  height: 100%;
  margin: 0 10rpx;
  display: inline-block;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  position: relative;
}
#indexWrap .header-wrap .nav-list .nav-item.active {
  color: #d43c33;
}
#indexWrap .header-wrap .nav-list .nav-item.active text::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 4rpx;
  background-color: #d43c33;
}
#indexWrap .content-wrap {
  width: 100%;
  margin-top: 164rpx;
}
#indexWrap .content-wrap .swiper {
  width: 100%;
  height: 370rpx;
}
#indexWrap .content-wrap .swiper swiper-item >image {
  width: 100%;
  height: 100%;
}
#indexWrap .content-wrap .service-policy {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
#indexWrap .content-wrap .service-policy .policy-item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
}
#indexWrap .content-wrap .service-policy .policy-item ._img {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 6rpx;
}
#indexWrap .content-wrap .service-policy .policy-item ._span {
  font-size: 24rpx;
  vertical-align: middle;
}
#indexWrap .content-wrap .shopList-nav {
  width: 100%;
  height: 340rpx;
  padding-bottom: 32rpx;
  background-color: #fff;
}
#indexWrap .content-wrap .shopList-nav >._ul {
  width: 100%;
  height: 100%;
}
#indexWrap .content-wrap .shopList-nav >._ul >._li {
  margin: 10rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 20%;
  height: 156rpx;
  float: left;
}
#indexWrap .content-wrap .shopList-nav >._ul >._li >._img {
  width: 110rpx;
  height: 110rpx;
  border-radius: 30rpx;
}
#indexWrap .content-wrap .shopList-nav >._ul >._li ._span {
  margin-top: 10rpx;
  font-size: 24rpx;
}
#indexWrap .content-wrap .promotion-module {
  width: 100%;
  background-color: #fff;
}
#indexWrap .content-wrap .promotion-module .floor-top,
#indexWrap .content-wrap .promotion-module position relative,
#indexWrap .content-wrap .promotion-module ._img {
  width: 100%;
  height: 240rpx;
}
#indexWrap .content-wrap .promotion-module .floor-first ._img {
  width: 100%;
  height: 160rpx;
}
#indexWrap .content-wrap .promotion-module .floor-first,
#indexWrap .content-wrap .promotion-module .floor-list {
  padding: 0 20rpx;
  background-color: #67a560;
}
#indexWrap .content-wrap .promotion-module .floor-list {
  width: 100%;
  overflow: hidden;
  padding-bottom: 20rpx;
}
#indexWrap .content-wrap .promotion-module .floor-list ._ul {
  width: 100%;
}
#indexWrap .content-wrap .promotion-module .floor-list ._ul ._li {
  width: 350rpx;
  height: 186rpx;
  margin-top: 10rpx;
  float: left;
}
#indexWrap .content-wrap .promotion-module .floor-list ._ul ._li ._img {
  width: 100%;
  height: 100%;
}
#indexWrap .content-wrap .promotion-module .floor-list ._ul ._li:nth-child(2n+1) {
  margin-right: 10rpx;
}

